{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Plugin",
        "program": "${workspaceFolder}/src/main.js",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "node",
        "console": "integratedTerminal",
        "outFiles": [
          "${workspaceFolder}/out/**/*.js"
        ],
        "env": {
          "NODE_ENV": "development"
        }
      }
    ]
  }